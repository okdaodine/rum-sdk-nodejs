{
  "name": "rum-sdk-nodejs",
  "version": "1.1.20",
  "license": "MIT",
  "description": "rum sdk for nodejs",
  "author": "rum",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/rum-sdk-nodejs.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "dev": "parcel index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/rum-sdk-nodejs.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/rum-sdk-nodejs.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/node-localstorage": "^1.3.0",
    "@types/uuid": "^8.3.4",
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "ethereumjs-util": "7.1.0",
    "ethers": "^5.6.9",
    "js-base64": "^3.7.2",
    "node-localstorage": "^2.2.1",
    "protobufjs": "^6.11.3",
    "query-string": "7.1.3",
    "store2": "^2.13.2",
    "uuid": "^8.3.2"
  }
}
